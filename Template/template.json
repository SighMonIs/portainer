        {"default": "51821", "label": "WireGuard VPN Port", "name": "PORT"},
        {"default": "51820", "label": "WireGuard WebUI Port", "name": "WG_PORT"},
        {"default": "1.1.1.1", "label": "Default DNS", "name": "WG_DEFAULT_DNS", "description": "Don't change this"},
        {"default": "10.8.0.x", "label": "IP Address Range", "name": "WG_DEFAULT_ADDRESS"},
        {"default": "0.0.0..0/0, ::/0", "label": "Allowed IP Addresses", "name": "WG_ALLOWED_IPS"}
      ],
      "volumes": [
        {"bind": "/docker/wg-easy", "container": "/etc/wireguard"}
      ],
      "cap_add": ["NET_ADMIN", "SYS_MODULE"],
      "sysctls": ["net.ipv4.ip_forward=1", "net.ipv4.conf.all.src_valid_mark=1"],
