{
  "version": "2",
  "templates": [
    {
      "categories": ["All"],
      "title": "Organizr",
      "logo": "https://raw.githubusercontent.com/SighMonIs/portainer/main/Images/organizr.png",
      "description": "Organizr allows you to setup Tabs that will be loaded all in one webpage. You can then work on your server with ease.",
      "type": 1,
      "platform": "linux",
      "name": "Organizr",
      "image": "organizr/organizr:latest",
      "ports": ["9949:80/tcp"],
      "env": [
        {"default": "1000", "label": "PUID", "name": "PUID"},
        {"default": "1000", "label": "PGID", "name": "PGID"},
        {"default": "Australia/Melbourne", "label": "TZ", "name": "TZ"}
      ],
      "volumes": [
        {"bind": "/portainer/Files/AppData/Config/organizr", "container": "/config"}
      ],
      "restart_policy": "unless-stopped"
    },
    {
      "categories": ["All"],
      "title": "Overseerr",
      "logo": "https://raw.githubusercontent.com/SighMonIs/portainer/main/Images/overseerr.png",
      "description": "Overseerr is a free and open source software application for managing requests for your media library.", 
      "type": 1,
      "platform": "linux",
      "name": "Overseerr",
      "image": "lscr.io/linuxserver/overseerr:latest",
      "ports": ["5055:5055/tcp"],
      "env": [
        {"default": "1000", "label": "PUID", "name": "PUID"},
        {"default": "1000", "label": "PGID", "name": "PGID"},
        {"default": "Australia/Melbourne", "name": "TZ", "label": "TZ"}
      ],
      "volumes": [
        {"bind": "/portainer/Files/AppData/Config/overseerr", "container": "/config"}
      ],
      "restart_policy": "unless-stopped"
    },
    {
      "categories": ["All"],
      "title": "qBittorrent",
      "logo": "https://raw.githubusercontent.com/SighMonIs/portainer/main/Images/qbittorrent.png",
      "description": "The qBittorrent project aims to provide an open-source software alternative to Torrent. qBittorrent is based on the Qt toolkit and libtorrent-rasterbar library.",
      "type": 1,
      "platform": "linux",
      "name": "qBittorrent",
      "image": "lscr.io/linuxserver/qbittorrent:latest",
      "ports": [
        {"8080:8080/tcp"},
        {"6881:6881/tcp"},
        {"6881:6881/udp"}
      ],
      "env": [
        {"defaults": "1000", "label": "PUID", "name": "PUID"},
        {"defaults": "1000", "label": "PGID", "name": "PGID"},
        {"defaults": "Australia/Melbourne", "label": "TZ", "name": "TZ"},
        {"defaults": "8080", "label": "WEBUI_PORT", "name": ""},
        {"defaults": "6881", "label": "TORRENTING_PORT", "name": "TORRENTING_PORT"}
      ],
      "volumes": [
        {"bind": "/path/to/appdata/config", "container": "/config"},
        {"bind": "/path/to/downloads", "container": "/downloads"}
      ],
      "restart_policy": "unless-stopped"
    }
  ]
}
